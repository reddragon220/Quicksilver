<window id="101">
	<defaultcontrol always="true">10</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<controls>
		<include>dialog_overlay</include>
		<control type="group">
		<!--
		<include>info_fade</include>
		-->
		<include>dialog_animation</include>
		<posy>-70</posy>
		<control type="image">
			<posx>560</posx>
			<posy>280</posy>
			<width>160</width>
			<height>161</height>
			<texture>dialog/busy/plex.png</texture>
			<aspectratio>stretch</aspectratio>
		</control>
		<control type="progress" id="20">
			<description>Progress Bar</description>
			<posx>257</posx>
			<posy>440</posy>
			<width>766</width>
			<height>22</height>
			<visible>true</visible>
			<reveal>true</reveal>
			<texturebg>dialog/progress/progress_bg.png</texturebg>
			<midtexture>dialog/progress/progress_center.png</midtexture>
			<lefttexture>dialog/progress/progress_left.png</lefttexture>
			<righttexture>dialog/progress/progress_right.png</righttexture>
			<overlaytexture>-</overlaytexture>
			<aspectratio>keep</aspectratio>
		</control>
		<control type="label" id="2">
			<posy>462</posy>
			<width>1280</width>
			<font>Context</font>
			<textcolor>ffffffff</textcolor>
			<align>center</align>
			<label>-</label>
		</control>
		<control type="label" id="3">
			<posy>496</posy>
			<width>1280</width>
			<font>Context</font>
			<textcolor>ffffffff</textcolor>
			<align>center</align>
			<label>-</label>
		</control>
		<control type="label" id="4">
			<posy>530</posy>
			<width>1280</width>
			<font>Context</font>
			<textcolor>ffffffff</textcolor>
			<align>center</align>
			<label>-</label>
		</control>
		<control type="button" id="10">
			<description>Cancel</description>
			<posx>554</posx>
			<posy>572</posy>
			<width>174</width>
			<height>50</height>
			<textcolor>ffffffff</textcolor>
			<font>ContextHeader</font>
			<label>$LOCALIZE[222]</label>
			<texturefocus>buttons/button/button_focus.png</texturefocus>
			<texturenofocus>buttons/button/button.png</texturenofocus>
			<aligny>bottom</aligny>
			<textoffsety>-8</textoffsety>
			<onleft>10</onleft>
			<onright>10</onright>
			<onup>10</onup>
			<ondown>10</ondown>
			<onclick>Dialog.Close(101)</onclick>
		</control>
		</control>
	</controls>
</window>
