          <width>280</width>
          <align>right</align>
          <textcolor>White</textcolor>
          <info>ListItem.Label2</info>
          <scroll>false</scroll>
        </control>
      </itemlayout>
      <focusedlayout height="32" width="320">
        <control type="image">
          <width>320</width>
          <height>30</height>
          <texture>ButtonFo.png</texture>
          <aspectratio aligny="middle">keep</aspectratio>
