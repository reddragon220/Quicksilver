          <posx>0</posx>
          <posy>0</posy>
          <width>320</width>
          <height>30</height>
          <texture>ButtonNF.png</texture>
          <aspectratio aligny="middle">keep</aspectratio>
        </control>
        <control type="image">
          <posx>5</posx>
          <posy>5</posy>
          <width>20</width>
          <height>20</height>
          <info>ListItem.Icon</info>
