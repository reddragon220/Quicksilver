<window id="125">
	<defaultcontrol always="true">1</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<coordinates>
		<system>1</system>
		<posx>100</posx>
		<posy>80</posy>
	</coordinates>
	<include>dialog_animation</include>
	<controls>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>848</width>
			<height>500</height>
			<texture>osd/bookmarks/background.png</texture>
			<aspectratio>stretch</aspectratio>
		</control>
		<control type="label">
			<include>DialogID</include>
			<label>VideoOSDBookmarks.xml</label>
		</control>
		<control type="grouplist" id="1">
			<itemgap>0</itemgap>
			<posx>662</posx>
			<posy>16</posy>
			<onleft>2</onleft>
			
			<onup>1</onup>
			<ondown>1</ondown>
			<onleft>11</onleft>
			<onright>11</onright>
			<control type="button" id="2">
				<description>Create Button</description>
				<width>181</width>
				<height>47</height>
				<font>SideButtons</font>
				<textcolor>ffffffff</textcolor>
				<label>Create</label>
				<texturefocus>dialog/pluginsettings/side_button_highlight.png</texturefocus>
				<texturenofocus>dialog/pluginsettings/side_button.png</texturenofocus>
				
				<textoffsety>-12</textoffsety>
				<aligny>bottom</aligny>
			</control>
			<control type="button" id="3">
				<description>Clear Button</description>
				<width>181</width>
				<height>47</height>
				<font>SideButtons</font>
				<textcolor>ffffffff</textcolor>
				<label>Clear</label>
				<texturefocus>dialog/pluginsettings/side_button_highlight.png</texturefocus>
				<texturenofocus>dialog/pluginsettings/side_button.png</texturenofocus>
				
				<textoffsety>-12</textoffsety>
				<aligny>bottom</aligny>
			</control>			
		</control>
		<control type="thumbnailpanel" id="11">
			<description>Bookmarks Panel</description>
			<posx>10</posx>
			<posy>40</posy>
			<width>639</width>
			<height>472</height>
			<itemwidth>100</itemwidth>
			<itemheight>110</itemheight>
			<texturewidth>90</texturewidth>
			<textureheight>80</textureheight>
			<thumbwidth>80</thumbwidth>
			<thumbheight>70</thumbheight>
			<thumbposx>5</thumbposx>
			<thumbposy>5</thumbposy>
			<onleft>2</onleft>
			<onright>11</onright>
			<ondown>11</ondown>
			<onup>11</onup>
			<imagefolder>-</imagefolder>
			<imagefolderfocus>button_overlay.png</imagefolderfocus>
		</control>
	</controls>
</window>
