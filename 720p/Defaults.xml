<includes>
  
  <default type="label">
    <posx>0</posx>
    <posy>0</posy>
    <label>-</label>
    <height>30</height>
    <align>left</align>
    <aligny>center</aligny>
    <font>Small</font>
    <textcolor>Black</textcolor>
    <focusedcolor>White</focusedcolor>
    <selectedcolor>White</selectedcolor>
    <disabledcolor>Red</disabledcolor>
    <scroll>true</scroll>
  </default>

  <default type="image">
    <posx>0</posx>
    <posy>0</posy>
    <width>-</width>
    <height>-</height>
    <align>center</align>
    <aligny>center</aligny>
    <texture>-</texture>
    <aspectratio>keep</aspectratio>
  </default>

  <default type="button">
    <posx>0</posx>
    <posy>0</posy>
    <width>150</width>
    <height>30</height>
    <align>center</align>
    <aligny>center</aligny>
    <font>Small</font>
    <label>-</label>
    <textcolor>Black</textcolor>
    <focusedcolor>White</focusedcolor>
    <disabledcolor>7f7c7c72</disabledcolor>
    <texturefocus>ButtonFo.png</texturefocus>
    <texturenofocus>ButtonNF.png</texturenofocus>
    <pulseonselect>false</pulseonselect>
  </default>

  <default type="togglebutton">
    <posx>0</posx>
    <posy>0</posy>
    <width>150</width>
    <height>30</height>
    <label>-</label>
    <font>Small</font>
    <textcolor>Black</textcolor>
    <focusedcolor>White</focusedcolor>
    <disabledcolor>7f7c7c72</disabledcolor>
    <texturenofocus>ButtonNF.png</texturenofocus>
    <texturefocus>ButtonFo.png</texturefocus>
    <alttexturenofocus>ButtonNF.png</alttexturenofocus>
    <alttexturefocus>ButtonFo.png</alttexturefocus>
    <align>center</align>
    <aligny>center</aligny>
    <pulseonselect>false</pulseonselect>
  </default>

  <default type="radiobutton">
    <posx>0</posx>
    <posy>0</posy>
    <width>150</width>
    <height>30</height>
    <align>center</align>
    <aligny>center</aligny>
    <font>Small</font>
    <label>-</label>
    <textcolor>Black</textcolor>
    <focusedcolor>White</focusedcolor>
    <disabledcolor>7f7c7c72</disabledcolor>
    <texturenofocus>ButtonNF.png</texturenofocus>
    <texturefocus>ButtonFo.png</texturefocus>
    <textureradiofocus>RadiobuttonFo.png</textureradiofocus>
    <textureradionofocus>RadiobuttonNF.png</textureradionofocus>
    <textoffsetx>5</textoffsetx>
    <pulseonselect>false</pulseonselect>
  </default>

  <default type="spincontrolex">
    <posx>0</posx>
    <posy>0</posy>
    <width>150</width>
    <height>30</height>
    <label>-</label>
    <font>Small</font>
    <texturenofocus>ButtonNF.png</texturenofocus>
    <texturefocus>ButtonFo.png</texturefocus>
    <textureup>ArrowUpNF.png</textureup>
    <texturedown>ArrowDownNF.png</texturedown>
    <textureupfocus>ArrowUpFo.png</textureupfocus>
    <texturedownfocus>ArrowDownFo.png</texturedownfocus>
    <textcolor>Black</textcolor>
    <focusedcolor>White</focusedcolor>
    <disabledcolor>7f7c7c72</disabledcolor>
    <textoffsetx>5</textoffsetx>
    <align>left</align>
    <aligny>center</aligny>
    <reverse>yes</reverse>
    <pulseonselect>false</pulseonselect>
  </default>
  
  <default type="spincontrol">
    <posx>0</posx>
    <posy>0</posy>
    <height>15</height>
    <width>15</width>
    <textureup>ArrowUpNF.png</textureup>
    <texturedown>ArrowDownNF.png</texturedown>
    <textureupfocus>ArrowUpFo.png</textureupfocus>
    <texturedownfocus>ArrowDownFo.png</texturedownfocus>
    <font>Small</font>
    <textcolor>Black</textcolor>
    <focusedcolor>White</focusedcolor>
    <disabledcolor>7f7c7c72</disabledcolor>
    <textoffsetx>5</textoffsetx>
    <align>left</align>
    <aligny>center</aligny>
    <reverse>yes</reverse>
    <pulseonselect>false</pulseonselect>
  </default>

  <default type="sliderex">
    <posx>0</posx>
    <posy>0</posy>
    <height>30</height>
    <width>150</width>
    <texturefocus>ButtonInputFo.png</texturefocus>
    <texturenofocus>ButtonInputNF.png</texturenofocus>
    <sliderwidth>100</sliderwidth>
    <sliderheight>15</sliderheight>
    <texturesliderbar>OSDSliderBack.png</texturesliderbar>
    <textureslidernib>OSDSliderNibNF.png</textureslidernib>
    <textureslidernibfocus>OSDSliderNibFO.png</textureslidernibfocus>
    <font>Small</font>
    <textcolor>Black</textcolor>
    <focusedcolor>White</focusedcolor>
    <disabledcolor>7f7c7c72</disabledcolor>
    <textoffsetx>5</textoffsetx>
    <textoffsety>0</textoffsety>
  </default>
  
  <default type="progress">
    <posx>0</posx>
    <posy>0</posy>
    <width>280</width>
    <height>15</height>
    <info>-</info>
    <texturebg>ProgressBack.png</texturebg>
    <lefttexture>ProgressLeft.png</lefttexture>
    <midtexture>ProgressMid.png</midtexture>
    <righttexture>ProgressRight.png</righttexture>
    <overlaytexture>-</overlaytexture>
  </default>

  <default type="slider">
    <posx>0</posx>
    <posy>0</posy>
    <width>280</width>
    <height>15</height>
    <texturesliderbar>ProgressBack.png</texturesliderbar>
    <textureslidernib>SeekBarSlider.png</textureslidernib>
    <textureslidernibfocus>SeekBarSlider.png</textureslidernibfocus>
  </default>

</includes>